{
  "name": "chefbook-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www.js",
    "mocha": "PORT=2999 NODE_ENV=test mocha --timeout 10000 --exit --",
    "test": "npm run test:all",
    "test:ci": "npm run test:all",
    "test:all": "npm run test:routes && npm run test:services && npm run test:models",
    "test:routes": "npm run mocha \"./src/routes/**/*.spec.js\"",
    "test:services": "npm run mocha \"./src/services/**/*.spec.js\"",
    "test:models": "npm run mocha \"./src/models/**/*.spec.js\""
  },
  "bin": "src/bin/www.js",
  "pkg": {
    "scripts": [
      "src/migrations/*",
      "src/models/*"
    ],
    "assets": [
      "src/public/stylesheets/*",
      "src/views/*",
      "node_modules/**/*"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "~8.4.0",
    "@julianpoy/recipe-clipper": "^2.3.0",
    "@sentry/node": "^7.13.0",
    "@sentry/tracing": "^7.13.0",
    "aws-sdk": "^2.1218.0",
    "body-parser": "~1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "express": "~4.18.1",
    "express-grip": "^1.2.1",
    "extract-zip": "^2.0.1",
    "firebase-admin": "^11.0.1",
    "fs-extra": "^10.1.0",
    "grip": "^1.5.0",
    "he": "^1.2.0",
    "ical-generator": "^3.5.1",
    "joi": "^17.6.4",
    "jsdom": "^20.0.0",
    "mdb": "git+https://git@github.com/julianpoy/node-mdb.git",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "v1.4.5-lts.1",
    "multer-imager": "git+https://git@github.com/julianpoy/multer-imager.git",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.6.7",
    "p-limit": "^3.1.0",
    "pg": "8.8.0",
    "pg-hstore": "^2.3.4",
    "pug": "^3.0.2",
    "puppeteer-core": "^17.1.3",
    "sanitize-html": "^2.7.2",
    "semver": "^7.3.7",
    "sequelize": "^6.23.0",
    "sharp": "^0.31.0",
    "sqlite3": "^5.1.1",
    "stripe": "^10.10.0",
    "umzug": "^3.2.1",
    "uuid": "^9.0.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.4.1",
    "sinon": "^14.0.0",
    "supertest": "^6.2.4"
  }
}
