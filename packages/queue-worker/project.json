{
  "name": "queue-worker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/queue-worker/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "main": "packages/queue-worker/src/index.ts",
        "outputPath": "dist/packages/queue-worker",
        "outputFileName": "main.js",
        "tsConfig": "packages/queue-worker/tsconfig.lib.json",
        "declaration": true,
        "assets": [
          {
            "glob": "packages/queue-worker/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "platform": "node"
      },
      "configurations": {
        "development": {
          "minify": false
        },
        "production": {
          "minify": true
        }
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/queue-worker",
        "command": "tsc --noEmit"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "maxWarnings": 0
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/packages/queue-worker"
      }
    }
  }
}
